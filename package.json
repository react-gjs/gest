{
  "name": "@reactgjs/gest",
  "version": "0.0.1",
  "main": "./dist/esm/user-land/index.mjs",
  "types": "./dist/types/user-land/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/esm/user-land/index.mjs",
      "types": "./dist/types/user-land/index.d.ts"
    },
    "./base-reporter": {
      "default": "./dist/esm/base/progress/base-reporter.mjs",
      "types": "./dist/types/base/progress/base-reporter.d.ts"
    },
    "./config": "./config.ts"
  },
  "bin": "./bin/gest.cjs",
  "packageManager": "yarn@3.4.1",
  "keywords": [
    "test",
    "testing",
    "unit",
    "gjs",
    "gnome",
    "javascript"
  ],
  "repository": {
    "url": "https://github.com/react-gjs/gest"
  },
  "description": "Testing framework for Gnome JavaScript (GJS).",
  "license": "MIT",
  "author": {
    "name": "Szymon Bretner",
    "email": "szymonb21@gmail.com"
  },
  "scripts": {
    "fix:lint": "eslint --fix .",
    "fix:prettier": "prettier -w ./src .",
    "test:lint": "eslint .",
    "test:prettier": "prettier -c ./src",
    "test:tsc": "tsc --noEmit",
    "build": "./scripts/build.mjs",
    "auto-bump": "./scripts/auto-bump.mjs",
    "publish:npm": "yarn build --clean && npm publish --access public"
  },
  "_bundledDependencies": [
    "dilswer",
    "termx-markup"
  ],
  "devDependencies": {
    "@ncpa0cpl/nodepack": "^2.1.2",
    "@types/node": "^18.15.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-hook-tasks": "ncpa0cpl/git-hook-tasks",
    "gjs-esm-types": "^0.0.3",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "prettier-plugin-jsdoc": "^0.4.2",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dilswer": "^2.0.2",
    "esbuild": "^0.17.17",
    "termx-markup": "^1.1.0"
  },
  "resolutions": {
    "dilswer": "2.0.2"
  }
}